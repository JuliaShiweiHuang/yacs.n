{
  "source": {
    "includePattern": ".+\\.(jsx|js|ts|tsx)$",
    "include": ["src"],
    "exclude": ["node_modules", "dist", "docs"]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "docs/",
    "recurse": true,
    "readme": "README.md",
    "encoding": "utf8"
  },
  "tags": {
    "allowUnknownTags": ["optional"]
  }
}
